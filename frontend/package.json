{
  "name": "studio-ollama-ui",
  "version": "1.0.0",
  "description": "Studio Ollama UI - Frontend for local Ollama models with web search and file access",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "server": "cd server && npm run dev",
    "electron": "electron .",
    "electron-dev": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && electron .\"",
    "electron-build": "npm run build && electron ."
  },
  "dependencies": {
    "axios": "^1.6.0",
    "highlight.js": "^11.11.1",
    "lucide-react": "^0.294.0",
    "markdown-it": "^13.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "rehype-highlight": "^7.0.2",
    "remark-gfm": "^4.0.1",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@types/markdown-it": "^13.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.0",
    "electron": "^27.0.0",
    "postcss": "^8.4.30",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "wait-on": "^7.1.0"
  }
}
